<!DOCTYPE html>
<html>
  <%= render "layouts/head"%>
  <body>
  <%= render 'layouts/header'%>
    <main>
      <div class="container">
      <%flash.each do |message_type,message|%>
        <%= content_tag(:div,message,class: "alert alert-#{message_type}")%>
      <%end%>
        <%= yield %>
      </div>
    </main>
  	<%= debug(params) if Rails.env.development? %>
		<%= render "layouts/footer"%>
  </body>
</html>

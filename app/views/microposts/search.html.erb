<section id="search">
    <p>検索</p>
    <%= form_tag(search_path, :method => "get") do %>
    <%= text_field_tag :search %>
    <%= submit_tag "Search",class: "submit", :name => nil %>
    <% end %>
    <% if !@microposts.nil? %>
    <ul><% @microposts.each do |micropost| %>
        <li>
            <%= link_to micropost.title, micropost %>
            <%= micropost.created_at %>
            <%= micropost.content %>
        </li>
        <% end %>
    </ul>
    <% end %>
</section>